{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "2015.csv"},
  "transform": [
    {"bin":true,
  "field": "Family",
  "as":"Binned Family"},
  {"bin":true,
  "field": "Generosity",
  "as":"Binned Generosity"}
  ],
  "mark": "rect",
  "encoding": {
    "y": {"field": "Binned Family", "bin":{
      "binned": true,
      "step": 0.2
    }},
    "x": {"field": "Binned Generosity", "bin":{
      "binned": true,
      "step": 0.2
    }},
    "color": {"aggregate":"count","field": "Trust"}
  },
  "config": {
    "axis": {"grid": true, "tickBand": "extent"}
  }
}